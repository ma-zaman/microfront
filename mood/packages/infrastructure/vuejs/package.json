{
  "name": "mood-gui",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "build:web": "webpack --config config/webpack.prod.js",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" -- &&  cp ./nginx.conf dist && cp ./mime.types dist",
    "preview": "vite preview",
    "test": "vitest --watch=false",
    "coverage": "vitest run --coverage",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test vite http://localhost:8090 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 8090' http://localhost:8090 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate-schema": "npx openapi-typescript src/configs/openapi.json -o src/configs/schema.d.ts"
  },
  "dependencies": {
    "@babel/preset-env": "^7.23.5",
    "@mdi/font": "^7.3.67",
    "@popperjs/core": "^2.11.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/vue3-jest": "^29.2.6",
    "axios": "^1.6.1",
    "babel-jest": "^29.7.0",
    "boosted": "^5.3.2",
    "dotenv-webpack": "^8.1.0",
    "oauth4webapi": "^2.10.3",
    "pinia": "^2.1.7",
    "postcss-loader": "^8.1.1",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "vue": "^3.3.10",
    "vue-i18n": "^9.8.0",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.3.3",
    "@testing-library/vue": "^8.0.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.3",
    "@vitejs/plugin-vue": "^4.5.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^5.0.0",
    "cypress": "^13.6.1",
    "eslint": "^8.55.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-vue": "^9.19.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "jsdom": "^23.0.1",
    "node-sass": "^9.0.0",
    "npm-run-all2": "^6.1.1",
    "openapi-typescript": "^6.7.2",
    "prettier": "^3.0.3",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.69.5",
    "sass-loader": "^13.0.0",
    "start-server-and-test": "^2.0.3",
    "style-loader": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.6",
    "vitest": "^0.34.6",
    "vue-style-loader": "^4.1.2",
    "vue-tsc": "^1.8.25",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  }
}
